{
  "name": "hoi-email-panel",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "db-generate": "drizzle-kit generate",
    "db-migrate": "drizzle-kit migrate",
    "db-push": "drizzle-kit push",
    "clean": "rm -rf .output && rm -rf .tanstack && rm -rf .nitro && rm -rf dist && rm -rf .vercel",
    "clean:full": "bun run clean && rm -rf node_modules && rm -rf bun.lockb",
    "clean:cache": "bun pm cache rm && rm -rf /tmp/bun-*",
    "clean:all": "bun run clean:full && bun run clean:cache",
    "deploy": "bun run clean && bun run vite build && bun run wrangler deploy",
    "cf-typegen": "wrangler types"
  },
